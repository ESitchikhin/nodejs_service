                        <path fill-rule="evenodd" clip-rule="evenodd" d="M20 3H19V1H17V3H7V1H5V3H4C2.9 3 2 3.9 2 5V21C2 22.1 2.9 23 4 23H20C21.1 23 22 22.1 22 21V5C22 3.9 21.1 3 20 3ZM4 21H20V5H4V21Z" fill="white"/>
                        <path d="M12 18H14V8H10V10H12V18Z" fill="white"/>
