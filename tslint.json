{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "forin": false,
        "no-bitwise": false,
        "jsdoc-format": false,
        "only-arrow-functions": false,
        "one-variable-per-declaration": false,
        "no-shadowed-variable": false,
        "variable-name": false,
        "prefer-for-of": false,
        "no-console": false
    },
    "rulesDirectory": []
}
